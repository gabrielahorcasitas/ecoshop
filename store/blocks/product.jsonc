  },
  "flex-layout.row#main-test": {
    "props": {
      "marginTop": 6
    },
    "children": [
      "flex-layout.col#left-test",
      "flex-layout.col#right-test"
    ]
  },
  "flex-layout.col#left-test": {
    "children": [
      "product-images"
    ]
  },
  "flex-layout.col#right-test": {
    "props": {
      "preventVerticalStretch": true,
      "verticalAlign": "middle"
    },
    "children": [
      "product-name",
      "add-to-list-btn",
      "link-seller",
      "product-identifier.product",
      "product-price",
      "flex-layout.row#qty-sku",
      "buy-button",
      "share#default"
    ]
  },
  "product-price": {
    "props": {
      "showSavings": true,
      "showListPrice": true
    }
  },
  "flex-layout.row#qty-sku": {
    "children": [
      "sku-selector",
      "product-quantity",
      "shipping-simulator"
    ]
  },
  "share#default": {
    "props": {
      "social": {
        "Facebook": true,
        "WhatsApp": true,
        "Twitter": false,
        "Pinterest": false
      }
    }
  }
}