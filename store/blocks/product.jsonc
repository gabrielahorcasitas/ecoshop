  "image#jabon-corporal":{
    "props": {
      "src" :"https://d3gr7hv60ouvr1.cloudfront.net/CACHE/images/products/ef050904-855d-47f7-af59-8429d136b548/56141243cf012b0110d22ee1761c7e2e.jpg",
      "maxHeight": 100,
      "link": {
        "url": "/jabon-corporal/p"
      }
